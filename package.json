{
  "name": "integrator-ui",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Celigo Inc.",
  "private": true,
  "license": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/celigo/integrator-ui.git"
  },
  "babel": {
    "plugins": [
      [
        "transform-es2015-modules-commonjs",
        {
          "useBuiltIns": true
        }
      ],
      "transform-object-rest-spread",
      "transform-async-to-generator",
      "babel-plugin-dynamic-import-node"
    ],
    "sourceMaps": true,
    "retainLines": true
  },
  "scripts": {
    "start": "neutrino start --require dotenv/config --options.config.devtool source-map",
    "build": "neutrino build --require dotenv/config",
    "lint": "neutrino lint  --require dotenv/config",
    "test": "neutrino test --require dotenv/config",
    "coverage": "neutrino test --coverage --require dotenv/config",
    "precommit": "neutrino test --no-cache --require dotenv/config && neutrino stage",
    "styleguide": "neutrino styleguide:start --require dotenv/config",
    "styleguide-build": "neutrino styleguide:build --require dotenv/config"
  },
  "dependencies": {
    "@material-ui/core": "4.3.2",
    "@material-ui/icons": "4.2.1",
    "@material-ui/lab": "4.0.0-alpha.23",
    "abortcontroller-polyfill": "1.3.0",
    "classnames": "2.2.6",
    "codemirror": "5.40.2",
    "deep-freeze": "0.0.1",
    "dexie": "2.0.4",
    "error-stack-parser": "2.0.2",
    "fast-json-patch": "2.0.7",
    "hookrouter": "1.2.3",
    "lodash": "4.17.14",
    "material-ui-popup-state": "1.3.2",
    "mdi-react": "5.2.0",
    "moment": "2.24.0",
    "notistack": "0.8.4",
    "prop-types": "15.6.2",
    "react": "16.8.6",
    "react-ace": "6.3.2",
    "react-codemirror2": "5.1.0",
    "react-copy-to-clipboard": "5.0.1",
    "react-diff-viewer": "1.1.0",
    "react-dom": "16.8.6",
    "react-forms-processor": "0.0.38",
    "react-fout-stager": "3.0.0",
    "react-hot-loader": "4.6.5",
    "react-json-view": "1.19.1",
    "react-loadable": "5.5.0",
    "react-redux": "7.1.0",
    "react-router-dom": "5.0.1",
    "react-select": "3.0.4",
    "react-timeago": "4.3.0",
    "redbox-react": "1.6.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.2",
    "redux-saga-requests": "0.23.0",
    "redux-saga-requests-fetch": "0.9.6",
    "shortid": "2.2.14",
    "typeface-roboto": "0.0.54",
    "clsx": "1.0.4"
  },
  "devDependencies": {
    "@neutrinojs/jest": "8.3.0",
    "@redux-saga/testing-utils": "1.0.5",
    "@testing-library/react": "8.0.2",
    "babel-plugin-dynamic-import-node": "2.3.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-register": "6.26.0",
    "dotenv": "6.2.0",
    "eslint-plugin-react-hooks": "^1.6.1",
    "jest-date-mock": "1.0.7",
    "jest-each": "24.8.0",
    "neutrino": "8.3.0",
    "neutrino-preset-mozilla-frontend-infra": "4.1.0",
    "prettier-eslint": "8.8.2"
  }
}
