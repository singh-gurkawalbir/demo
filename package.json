{
  "name": "integrator-ui",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Celigo Inc.",
  "private": true,
  "license": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/celigo/integrator-ui.git"
  },
  "jest": {
    "setupFiles": [
      "jest-date-mock"
    ]
  },
  "babel": {
    "plugins": [
      [
        "transform-es2015-modules-commonjs",
        {
          "useBuiltIns": true
        }
      ],
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ],
    "sourceMaps": true,
    "retainLines": true
  },
  "scripts": {
    "start": "neutrino start --require dotenv/config --options.config.devtool source-map",
    "build": "neutrino build --require dotenv/config",
    "lint": "neutrino lint  --require dotenv/config",
    "test": "neutrino test --require dotenv/config --verbose",
    "coverage": "neutrino test --coverage --require dotenv/config",
    "precommit": "neutrino test --require dotenv/config && neutrino stage",
    "styleguide": "neutrino styleguide:start --require dotenv/config"
  },
  "dependencies": {
    "@material-ui/core": "3.9.1",
    "@material-ui/icons": "3.0.2",
    "@material-ui/lab": "3.0.0-alpha.29",
    "abortcontroller-polyfill": "^1.3.0",
    "classnames": "2.2.6",
    "codemirror": "5.40.2",
    "dexie": "2.0.4",
    "error-stack-parser": "2.0.2",
    "fast-json-patch": "2.0.7",
    "integrator-ui-forms": "git+ssh://git@github.com/celigo/integrator-ui-forms.git",
    "lodash": "4.17.11",
    "material-ui-popup-state": "1.3.2",
    "mdi-react": "5.2.0",
    "moment": "2.24.0",
    "notistack": "^0.8.4",
    "prop-types": "15.6.2",
    "react": "16.7.0",
    "react-ace": "6.3.2",
    "react-codemirror2": "5.1.0",
    "react-dom": "16.7.0",
    "react-fout-stager": "3.0.0",
    "react-hot-loader": "4.6.5",
    "react-json-view": "^1.19.1",
    "react-loadable": "5.5.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-timeago": "4.3.0",
    "redbox-react": "1.6.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "1.0.2",
    "redux-saga-requests": "0.23.0",
    "redux-saga-requests-fetch": "0.9.6",
    "shortid": "2.2.14",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@neutrinojs/jest": "8.3.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-register": "6.26.0",
    "dotenv": "6.2.0",
    "jest-date-mock": "1.0.7",
    "neutrino": "8.3.0",
    "neutrino-preset-mozilla-frontend-infra": "4.1.0",
    "prettier-eslint": "8.8.2"
  }
}
