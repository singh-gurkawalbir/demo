{
  "name": "integrator-ui",
  "version": "1.0.0",
  "main": "index.js",
  "author": "Celigo Inc.",
  "license": "",
  "jest": {
    "setupFiles": [
      "jest-date-mock"
    ]
  },
  "babel": {
    "plugins": [
      [
        "transform-es2015-modules-commonjs",
        {
          "useBuiltIns": true
        }
      ],
      "transform-object-rest-spread",
      "transform-async-to-generator"
    ],
    "sourceMaps": true,
    "retainLines": true
  },
  "scripts": {
    "start": "neutrino start --require dotenv/config",
    "build": "neutrino build --require dotenv/config",
    "lint": "neutrino lint",
    "test": "neutrino test --require dotenv/config --verbose",
    "coverage": "neutrino test --coverage --require dotenv/config",
    "precommit": "neutrino test --coverage --require dotenv/config && neutrino stage",
    "styleguide": "neutrino styleguide:start --require dotenv/config"
  },
  "dependencies": {
    "@material-ui/core": "3.3.1",
    "@material-ui/icons": "3.0.1",
    "@material-ui/lab": "3.0.0-alpha.21",
    "classnames": "2.2.6",
    "codemirror": "5.40.2",
    "dexie": "2.0.4",
    "error-stack-parser": "2.0.2",
    "fast-json-patch": "2.0.7",
    "lodash": "4.17.11",
    "mdi-react": "4.4.0",
    "prop-types": "15.6.2",
    "react": "16.6.0",
    "react-ace": "6.2.0",
    "react-codemirror2": "5.1.0",
    "react-dom": "16.6.0",
    "react-fout-stager": "3.0.0",
    "react-hot-loader": "4.3.11",
    "react-loadable": "5.5.0",
    "react-redux": "5.0.7",
    "react-router-dom": "4.3.1",
    "react-timeago": "4.1.9",
    "redbox-react": "1.6.0",
    "redux": "4.0.1",
    "redux-logger": "3.0.6",
    "redux-saga": "0.16.2",
    "shortid": "2.2.13",
    "typeface-roboto": "0.0.54"
  },
  "devDependencies": {
    "@babel/plugin-syntax-optional-chaining": "^7.2.0",
    "@neutrinojs/jest": "8.3.0",
    "babel-plugin-transform-async-to-generator": "^6.16.0",
    "babel-register": "6.26.0",
    "dotenv": "6.2.0",
    "jest-date-mock": "1.0.6",
    "neutrino": "8.3.0",
    "neutrino-preset-mozilla-frontend-infra": "4.1.0",
    "prettier-eslint": "8.8.2"
  }
}
